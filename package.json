{
  "name": "typography-analyzer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.13",
    "@mui/utils": "^5.14.13",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.58",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build": "INLINE_RUNTIME_CHUNK=false craco build",
    "watch": "npm-watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint src/ --ext .ts,.tsx --max-warnings=0",
    "lint:prettier": "prettier 'src/**/*.{ts,tsx}' --check",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint src/ --ext .ts,.tsx --fix --max-warnings=0",
    "fix:prettier": "prettier 'src/**/*.{ts,tsx}' --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx,css"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chrome": "^0.0.246",
    "craco": "^0.0.3",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.0",
    "npm-watch": "^0.11.0",
    "prettier": "^3.0.3"
  }
}
